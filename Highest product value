import java.util.*;

public class Main {
    public static void main(String[] args) {
      Scanner S=new Scanner(System.in);
      int a=8;
      if(a==2) System.out.println(1);
      if(a==3) System.out.println(2);
      int p=1;
      while(a>4)
      {
        p=p*3;
        a=a-3;
        System.out.print(3+"*");
      }
      p=p*a;
      System.out.println(a);
      System.out.println(p);
  }
}
