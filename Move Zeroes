import java.util.*;

public class Main {
    public static void main(String[] args) {
     
        int a=0;
        int nums[]={0,1,0,3,12};
        for(int i=0;i<nums.length;i++) {
            if(nums[i]!=0) 
            {
                nums[a]=nums[i];
                a++;
            }
        }
        for(int i=a;i<nums.length;i++) {
            nums[i]=0;
        }
        for(int i=0;i<nums.length;i++) 
        {
          System.out.print(nums[i]+" ");
        }
        
    }
}
