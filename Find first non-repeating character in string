import java.util.*;

public class Main {
    public static void main(String[] args) {
      Scanner S=new Scanner(System.in);
      String str=S.nextLine();
      for(int i=0;i<str.length();i++)
      {
        int c=0;
        for(int j=0;j<str.length();j++)
        {
          if(str.charAt(i)==str.charAt(j))
          {
            c++;
          }
        }
        if(c==1)
        {
          System.out.print(str.charAt(i));
          return;
        }
        
      }
  }
}
